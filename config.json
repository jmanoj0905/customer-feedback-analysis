{
  "aws": {
    "region": "us-east-1",
    "profile": "default"
  },
  "lambda": {
    "function_name": "CustomerFeedbackAnalyzer",
    "runtime": "python3.9",
    "timeout": 60,
    "memory_size": 256,
    "handler": "lambda_function.lambda_handler",
    "role_name": "CustomerFeedbackLambdaRole"
  },
  "dynamodb": {
    "table_name": "CustomerFeedback",
    "read_capacity": 5,
    "write_capacity": 5,
    "sort_key_enabled": true
  },
  "s3": {
    "bucket_name": "customer-feedback-analysis-bucket",
    "website_bucket": "customer-feedback-website"
  },
  "api_gateway": {
    "api_name": "CustomerFeedbackAPI",
    "stage_name": "prod",
    "endpoint_url": "YOUR_API_GATEWAY_URL_HERE"
  },
  "comprehend": {
    "language_code": "en",
    "max_text_length": 5000
  },
  "frontend": {
    "title": "Customer Feedback Analysis System",
    "use_mock_data": true
  }
}
